<!DOCTYPE html>
<html lang="{{lang}}">
<head>
    <!-- COMPONENT: GTM-DATALAYER -->
    <!-- COMPONENT: GTM-HEAD -->
    <!-- COMPONENT: METADATA -->
    <!-- COMPONENT: RESOURCE-HINTS -->
    
    <!-- Font optimization to prevent layout shifts -->
    <style>
        /* System font stack optimization */
        body {
            font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
        }
        
        /* Critical CSS will be injected here */
    </style>
    
    <!-- COMPONENT: STRUCTURED-DATA -->
</head>
<body>
    <!-- COMPONENT: GTM-BODY -->
    
    <div class="d-flex flex-column min-vh-100">
        <!-- COMPONENT: NAVBAR -->
        
        <main class="flex-grow-1">
            <!-- Hero Section -->
            <section class="hero-section position-relative">
                <div class="hero-image-container">
                    <picture>
                        <source media="(max-width: 576px)" srcset="../img/gallery/out_front_right_400w.webp" type="image/webp">
                        <source media="(max-width: 1200px)" srcset="../img/gallery/out_front_right_800w.webp" type="image/webp">
                        <source media="(min-width: 1201px)" srcset="../img/gallery/out_front_right_1200w.webp" type="image/webp">
                        <img src="../img/gallery/out_front_right_800w.webp" alt="{{hero_image_alt}}" class="hero-image img-fluid" loading="eager" fetchpriority="high" data-translate-key-alt="hero_image_alt">
                    </picture>
                </div>
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 hero-content">
                            <h1 class="display-4 fw-bold text-white mb-4" data-translate-key="hero_title">{{hero_title}}</h1>
                            <p class="lead text-white mb-4" data-translate-key="hero_subtitle">{{hero_subtitle}}</p>
                            <a href="#booking" class="btn btn-primary btn-lg px-4 me-md-2" data-translate-key="hero_cta">{{hero_cta}}</a>
                        </div>
                    </div>
                </div>
            </section>
            
            <!-- Features Section -->
            <section class="py-5 bg-light">
                <div class="container">
                    <h2 class="text-center mb-5" data-translate-key="features_title">{{features_title}}</h2>
                    <div class="row g-4">
                        <div class="col-md-4">
                            <div class="card h-100 border-0 shadow-sm">
                                <div class="card-body text-center">
                                    <i class="bi bi-house-door fs-1 text-primary mb-3"></i>
                                    <h3 class="card-title h5" data-translate-key="feature_1_title">{{feature_1_title}}</h3>
                                    <p class="card-text" data-translate-key="feature_1_description">{{feature_1_description}}</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card h-100 border-0 shadow-sm">
                                <div class="card-body text-center">
                                    <i class="bi bi-gear fs-1 text-primary mb-3"></i>
                                    <h3 class="card-title h5" data-translate-key="feature_2_title">{{feature_2_title}}</h3>
                                    <p class="card-text" data-translate-key="feature_2_description">{{feature_2_description}}</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card h-100 border-0 shadow-sm">
                                <div class="card-body text-center">
                                    <i class="bi bi-map fs-1 text-primary mb-3"></i>
                                    <h3 class="card-title h5" data-translate-key="feature_3_title">{{feature_3_title}}</h3>
                                    <p class="card-text" data-translate-key="feature_3_description">{{feature_3_description}}</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            
            <!-- Gallery Section -->
            <section class="py-5">
                <div class="container">
                    <h2 class="text-center mb-5" data-translate-key="gallery_title">{{gallery_title}}</h2>
                    <div class="row g-4">
                        <div class="col-md-4">
                            <img src="../img/gallery/in_right_corridor_closedbed_800w.webp" alt="{{gallery_alt_1}}" class="img-fluid rounded shadow" loading="lazy" data-translate-key-alt="gallery_alt_1">
                        </div>
                        <div class="col-md-4">
                            <img src="../img/gallery/in_kitchen_800w.webp" alt="{{gallery_alt_2}}" class="img-fluid rounded shadow" loading="lazy" data-translate-key-alt="gallery_alt_2">
                        </div>
                        <div class="col-md-4">
                            <img src="../img/gallery/in_left_openbed_800w.webp" alt="{{gallery_alt_3}}" class="img-fluid rounded shadow" loading="lazy" data-translate-key-alt="gallery_alt_3">
                        </div>
                    </div>
                </div>
            </section>
            
            <!-- Booking Section -->
            <section id="booking" class="py-5 bg-light">
                <div class="container">
                    <h2 class="text-center mb-5" data-translate-key="booking_title">{{booking_title}}</h2>
                    <div class="row justify-content-center">
                        <div class="col-lg-8">
                            <div class="card border-0 shadow">
                                <div class="card-body p-4">
                                    <p class="text-center mb-4" data-translate-key="booking_description">{{booking_description}}</p>
                                    <div class="d-grid gap-2">
                                        <a href="mailto:info@tartuhaagissuvila.ee" class="btn btn-primary btn-lg" data-translate-key="booking_cta_email">{{booking_cta_email}}</a>
                                        <a href="tel:+3725512345" class="btn btn-outline-primary btn-lg" data-translate-key="booking_cta_phone">{{booking_cta_phone}}</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            
            <!-- FAQ Section -->
            <section class="py-5">
                <div class="container">
                    <h2 class="text-center mb-5" data-translate-key="faq_title">{{faq_title}}</h2>
                    <div class="row justify-content-center">
                        <div class="col-lg-8">
                            <div class="accordion" id="faqAccordion">
                                <div class="accordion-item">
                                    <h3 class="accordion-header">
                                        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#faq1" aria-expanded="true" aria-controls="faq1" data-translate-key="faq_question_min_rental">
                                            {{faq_question_min_rental}}
                                        </button>
                                    </h3>
                                    <div id="faq1" class="accordion-collapse collapse show" data-bs-parent="#faqAccordion">
                                        <div class="accordion-body" data-translate-key="faq_answer_min_rental">
                                            {{faq_answer_min_rental}}
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item">
                                    <h3 class="accordion-header">
                                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#faq2" aria-expanded="false" aria-controls="faq2" data-translate-key="faq_question_prices">
                                            {{faq_question_prices}}
                                        </button>
                                    </h3>
                                    <div id="faq2" class="accordion-collapse collapse" data-bs-parent="#faqAccordion">
                                        <div class="accordion-body" data-translate-key="faq_answer_prices">
                                            {{faq_answer_prices}}
                                        </div>
                                    </div>
                                </div>
                                <div class="accordion-item">
                                    <h3 class="accordion-header">
                                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#faq3" aria-expanded="false" aria-controls="faq3" data-translate-key="faq_question_license">
                                            {{faq_question_license}}
                                        </button>
                                    </h3>
                                    <div id="faq3" class="accordion-collapse collapse" data-bs-parent="#faqAccordion">
                                        <div class="accordion-body" data-translate-key="faq_answer_license">
                                            {{faq_answer_license}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            
            <!-- Contact Section -->
            <section class="py-5 bg-light">
                <div class="container">
                    <h2 class="text-center mb-5" data-translate-key="contact_title">{{contact_title}}</h2>
                    <div class="row justify-content-center">
                        <div class="col-md-6 mb-4 mb-md-0">
                            <div class="card h-100 border-0 shadow">
                                <div class="card-body p-4">
                                    <h3 class="h5 mb-3" data-translate-key="contact_info_title">{{contact_info_title}}</h3>
                                    <p class="mb-2"><i class="bi bi-envelope me-2"></i> <a href="mailto:info@tartuhaagissuvila.ee">info@tartuhaagissuvila.ee</a></p>
                                    <p class="mb-2"><i class="bi bi-telephone me-2"></i> <a href="tel:+3725512345">+372 551 2345</a></p>
                                    <p class="mb-0"><i class="bi bi-geo-alt me-2"></i> <span data-translate-key="contact_address">{{contact_address}}</span></p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="card h-100 border-0 shadow">
                                <div class="card-body p-4">
                                    <h3 class="h5 mb-3" data-translate-key="contact_location_title">{{contact_location_title}}</h3>
                                    <div class="ratio ratio-16x9">
                                        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2091.4365235583147!2d26.70984307680702!3d58.37734747332396!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x46eb36d8d55d3d45%3A0x73270c26ee46c990!2sTartu%2C%20Estonia!5e0!3m2!1sen!2sus!4v1689252525895!5m2!1sen!2sus" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </main>
        
        <!-- COMPONENT: FOOTER -->
    </div>
    
    <!-- Bootstrap JS Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous" defer></script>
</body>
</html>